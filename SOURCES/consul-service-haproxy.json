{
    "service": {
        "name": "haproxy",
        "port": 80,
        "tags": [
            "haproxy",
            "lb"
        ],
        "checks": [
            {
                "name": "stats",
                "http": "http://admin:admin@localhost/haproxy?stats",
                "interval": "10s"
            },
            {
                "name": "default backend",
                "http": "http://localhost.ena.com/",
                "interval": "10s"
            }
        ]
    }
}
